{
  "tabnine.experimentalAutoImports": true,
  "restructuredtext.languageServer.disabled": true,
  "ui.zoom": 1.0,
  "restructuredtext.confPath": "",
  "restructuredtext.languageServer.disabled": true,
  "workbench.colorTheme": "One Dark Pro",
  "editor.detectIndentation": true,
  "editor.fontFamily": "DejaVu Sans Code",
  "editor.fontSize": 14,
  "editor.fontLigatures": true,
  "editor.largeFileOptimizations": true,
  "editor.highlightActiveIndentGuide": true,
  "editor.indentSize": 2,
  "editor.insertSpaces": true,
  "editor.lineNumbers": "on",
  "editor.matchBrackets": true,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "none",
  "editor.rulers": [],
  "editor.tabSize": 4,
  "editor.formatOnSave": true,
  "editor.zenMode.hideTabs": true,
  "editor.zenMode.singleFile": true,
  "editor.lightBulb.enabled": true,
  "files.exclude": [ "_esy", ".git" ],
  "files.useExperimentalFileWatcher": true,
  "terminal.integrated.fontFamily": "DejaVuSansMono Nerd Font Mono",
  "terminal.integrated.shell.osx": "bash",
  "workbench.activityBar.visible": true,
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "vs-seti",
  "workbench.sideBar.location": "left",
  "workbench.sideBar.visible": true,
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 2,
  "oni.inactiveWindowOpacity": 0.8,
  "vim.useSystemClipboard": [ "yank" ],
  "vim.leader": "\\",
  "yarn.runInTerminal": false,
  "yarn.dontHideOutputOnSuccess": true,
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" }
}
